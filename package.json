{
  "name": "fhenix.js",
  "version": "0.2.1",
  "description": "The JavaScript SDK for Fhenix",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "files": [
    "dist",
    "src",
    "licenses",
    "package.json",
    "package-lock.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint src/",
    "build:prod": "webpack --config webpack/webpack.production.js",
    "build:dev": "webpack --config webpack/webpack.development.js --profile --json > stats.json",
    "serve": "webpack serve",
    "test": "vitest",
    "test-ci": "jest --forceExit --runInBand",
    "test:watch": "jest --colors --passWithNoTests --watch",
    "test-browser": "web-test-runner \"test-browser/**/*.test.js\" --node-resolve --puppeteer",
    "coverage": "vitest run --coverage",
    "prettier": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FhenixProtocol/fhenix.js.git"
  },
  "author": "",
  "license": "BSD-3-Clause-Clear",
  "bugs": {
    "url": "https://github.com/FhenixProtocol/fhenix.js/issues"
  },
  "homepage": "https://github.com/FhenixProtocol/fhenix.js#readme",
  "dependencies": {
    "@sindresorhus/is": "^6.1.0",
    "ethers": "^6.9.1",
    "libsodium": "^0.7.11",
    "libsodium-wrappers": "^0.7.11",
    "node-tfhe": "^0.3.1",
    "tfhe": "^0.3.1"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@types/libsodium-wrappers": "^0.7.10",
    "@types/node": "^18.11.3",
    "@vitest/coverage-istanbul": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "compression-webpack-plugin": "^10.0.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.4",
    "typedoc-plugin-extras": "^3.0.0",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  }
}
